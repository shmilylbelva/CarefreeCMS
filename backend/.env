# 开发环境配置文件
# 生产环境请复制 .env.example 并修改为实际配置

# 调试模式：true=开启（开发环境），false=关闭（生产环境）
APP_DEBUG = true

[APP]
# 时区设置
DEFAULT_TIMEZONE = Asia/Shanghai

[DATABASE]
# 数据库驱动
DB_DRIVER = mysql
DB_TYPE = mysql
# 数据库主机地址
DB_HOST = 127.0.0.1
# 数据库名称
DB_NAME = 
# 数据库用户名
DB_USER = 
# 数据库密码
DB_PASS = 
# 数据库端口
DB_PORT = 3306
# 数据库字符集
DB_CHARSET = utf8mb4
# 数据库表前缀
DB_PREFIX =

[LANG]
# 默认语言
DEFAULT_LANG = zh-cn

[JWT]
# JWT 密钥（请勿泄露此密钥！）
# JWT_SECRET = iC8fT2rH9bP5oF2bB0uY9lD6qQ7kG4iU
JWT_SECRET = iC8fT2rH9bP5oF2bB0uY9lD6qQ7kG4iU
# JWT 过期时间（秒）7200秒 = 2小时
JWT_EXPIRE = 7200

[CORS]
# CORS 允许的来源白名单（多个用逗号分隔）
# 开发环境允许本地前端访问
CORS_ALLOWED_ORIGINS = http://localhost:5173,http://localhost:3000,http://127.0.0.1:5173
