{
    "name": "sinma/carefreecms",
    "version": "2.0.0",
    "description": "逍遥内容管理系统",
    "type": "project",
    "keywords": [
        "cms",
        "content management",
        "carefree"
    ],
    "homepage": "https://www.sinma.net/",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "sinma",
            "email": "sinma@qq.com",
            "homepage": "https://www.sinma.net/"
        }
    ],
    "require": {
        "php": ">=8.0.0",
        "topthink/framework": "^8.0",
        "topthink/think-orm": "^3.0|^4.0",
        "topthink/think-filesystem": "^2.0|^3.0",
        "firebase/php-jwt": "^6.11",
        "topthink/think-view": "^2.0",
        "dragonmantank/cron-expression": "^3.6",
        "erusev/parsedown": "^1.7",
        "aliyuncs/oss-sdk-php": "^2.7",
        "qcloud/cos-sdk-v5": "^2.0",
        "qiniu/php-sdk": "^7.12",
        "php-ffmpeg/php-ffmpeg": "^1.2",
        "topthink/think-queue": "^3.0",
        "twig/twig": "^3.22",
        "phpoffice/phpspreadsheet": "^5.3",
        "zircote/swagger-php": "^5.7"
    },
    "require-dev": {
        "topthink/think-dumper": "^1.0",
        "topthink/think-trace": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "app\\": "app"
        },
        "psr-0": {
            "": "extend/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "scripts": {
        "post-autoload-dump": [
            "@php think service:discover",
            "@php think vendor:publish"
        ]
    }
}
